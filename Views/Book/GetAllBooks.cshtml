@{
    <table class="table table-hover">
    <tr>
        <th>BookId</th>
        <th>Title</th>
        <th>Description</th>
    </tr>
    @foreach(Book book in ViewBag.Books)
    {
        <tr>
            <td>@book.BookId</td>
            <td>@book.Title</td>
            <td>@book.Description</td>
        </tr>
    }
    </table>
}
